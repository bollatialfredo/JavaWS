
public class Nodo implements Comparable<Nodo> {
	
	private int valor;
	private Nodo der;
	private Nodo izq;
	
	public Nodo(int v){
		valor = v;
	}
	
	public int getValor(){
		return valor;
	}
	
	public void addNodo(Nodo n){
		int v = this.compareTo(n);
		if (v>0) {
			if (der!=null) {
				der.addNodo(n);
			}else{
				der = n;
			}
		}else{
			if (v<0) {
				if (izq!=null) {
					izq.addNodo(n);
				}else{
					izq = n;
				}
			}
		}
	}
	
	public void print(){
		if (izq!=null) {
			izq.print();
		}
		System.out.println(valor);
		if (izq!=null) {
			izq.print();
		}
	}
	
	@Override
	public int compareTo(Nodo n) {
		if (n.getValor()>valor) {
			return -1;
		}else{
			if(n.getValor()<valor){
				return 1;
			}
		}
		
		return 0;
	}

}
