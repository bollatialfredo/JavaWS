
public class ListaArreglo {
	String [] lista;
	int cantidad = 0;
	
	public ListaArreglo () {
		lista = new String[10];
	}
	
	public void AddElement(String s) {
		
		if(cantidad == lista.length) {
			String [] listaAux = lista;
			lista = new String[lista.length*2];
			for (int i = 0; i < listaAux.length; i++) {
				lista[i] = listaAux[i];
			}
		}//else No needed
		
		lista[cantidad] = s;
		cantidad++;
	}
	
 
	
}
