package nodoParcial;

import java.util.Iterator;
import java.util.*;

public class IteradorNodo implements Iterator<Comparable> {
	private List<Comparable> values = new ArrayList<Comparable>();
	Nodo nodo;
	
	public IteradorNodo(Nodo n){
		nodo = n;
		values.add(n.value());
		while (n.nodoSig!=null){
			values.add(n.nodoSig.value());
		}
	}
	
	@Override
	public boolean hasNext() {
		return nodo.haySiguiente();
	}

	@Override
	public Comparable next() {
		return nodo.nodoSig.value();
	}

}
