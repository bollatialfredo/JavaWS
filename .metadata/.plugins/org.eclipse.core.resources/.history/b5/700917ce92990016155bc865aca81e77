import java.util.ArrayList;
import java.util.List;

public class Pila {

	private List<Object> l = new ArrayList<Object>();
	
	public Object top(){
		return l.get(l.size()-1);
	}
	public Object pop(){
		Object o = top();
		l.remove(top());
		return o;
	}
	
	public static void main(String[] args) {
		Pila p = new Pila();

	}
}
