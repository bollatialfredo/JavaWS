import java.util.ArrayList;
import java.util.Collections;
import java.util.List;

public class IteradorDescendente extends IteradorNodo {
	
	
	public IteradorDescendente(Nodo no){
		lista = new ArrayList<Comparable>();
		lista.addAll(no.getValoresDer());
		lista.add(no.getValor());
		lista.addAll(no.getValoresIzq());
	}
	public List<Comparable> getValoresDer(){
		List<Comparable> lista = new ArrayList<Comparable>();
		if(nodoDer != null){
			lista.addAll(nodoDer.getValores());
		}
		return lista;
	} 
}
