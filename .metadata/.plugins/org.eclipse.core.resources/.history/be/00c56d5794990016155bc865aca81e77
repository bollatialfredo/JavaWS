import java.util.ArrayList;
import java.util.Collections;
import java.util.List;

public class Pila {

	private List<Object> l = new ArrayList<Object>();
	
	public Object top(){
		return l.get(l.size()-1);
	}
	public Object pop(){
		Object o = top();
		l.remove(top());
		return o;
	}
	
	public void push(Object o){
		l.add(o);
	}
	
	public Pila reverse(){
		Pila aux = new Pila();
		List<Object> aux2 = new ArrayList<Object>();
		aux2=l;
		Collections.reverse(aux2);
		for (Object o : aux2) {
			aux.push(o);
		}
		return aux;
	}
	
	public static void main(String[] args) {
		Pila p = new Pila();

	}
}
